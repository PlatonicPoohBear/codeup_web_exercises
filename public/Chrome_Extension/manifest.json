{
  "manifest_version": 2,

  "name": "Youtube Video Generator",
  "description": "This extension generates a random Youtube video.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "chrome_extension.png",
    "default_popup": "chrome_extension.html"
  },
  "permissions": [
    "activeTab",
    "https://www.googleapis.com/youtube/v3/search"
  ]
}